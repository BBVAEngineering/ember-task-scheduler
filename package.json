{
  "name": "ember-task-scheduler",
  "version": "2.1.1",
  "description": "An Ember CLI addon to schedule tasks to try to preserve 60 FPS rate on browsers.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BBVAEngineering/ember-task-scheduler.git"
  },
  "license": "MIT",
  "author": "BBVAEngineering",
  "directories": {
    "test": "tests"
  },
  "contributors": [
    {
      "name": "beatrizdemiguelperez",
      "url": "https://github.com/beatrizdemiguelperez/"
    },
    {
      "name": "adrigzr",
      "url": "https://github.com/adrigzr/"
    },
    {
      "name": "shokmaster",
      "url": "https://github.com/shokmaster/"
    },
    {
      "name": "josex2r",
      "url": "https://github.com/josex2r/"
    }
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 95 --functions 100 --branches 85 --lines 95",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.2",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "~1.4.0",
    "eslint": "^7.8.0",
    "eslint-config-bbva": "^2.0.3",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^3.0.1",
    "istanbul": "^0.4.5",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/BBVAEngineering/ember-task-scheduler/issues"
  },
  "homepage": "https://github.com/BBVAEngineering/ember-task-scheduler#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
