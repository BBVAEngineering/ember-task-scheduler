{
  "name": "ember-task-scheduler",
  "version": "1.0.5",
  "description": "An Ember CLI addon to schedule tasks to try to preserve 60 FPS rate on browsers.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BBVAEngineering/ember-task-scheduler.git"
  },
  "license": "MIT",
  "author": "BBVAEngineering",
  "directories": {
    "test": "tests"
  },
  "contributors": [
    {
      "name": "beatrizdemiguelperez",
      "url": "https://github.com/beatrizdemiguelperez/"
    },
    {
      "name": "adrigzr",
      "url": "https://github.com/adrigzr/"
    },
    {
      "name": "shokmaster",
      "url": "https://github.com/shokmaster/"
    },
    {
      "name": "josex2r",
      "url": "https://github.com/josex2r/"
    }
  ],
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "test:all": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 90 --functions 95 --branches 75 --lines 90",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@ember/optional-features": "^0.7.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "ember-cli": "~3.8.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-sinon": "^3.1.0",
    "ember-source": "~3.8.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "~0.2.23",
    "eslint": "^5.14.1",
    "eslint-config-bbva": "^2.0.3",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/BBVAEngineering/ember-task-scheduler/issues"
  },
  "homepage": "https://github.com/BBVAEngineering/ember-task-scheduler#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
